<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="433.0" prefWidth="646.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.CAM.GUI.Controller">
  <children>
    <MenuBar prefHeight="29.0" prefWidth="719.0" VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="File">
          <items>
            <MenuItem fx:id="menuSetup" mnemonicParsing="false" onAction="#setupAction" text="Setup" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem fx:id="menuDebug" mnemonicParsing="false" onAction="#toggleDebugAction" text="Toggle Debug" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem fx:id="menuGitHub" mnemonicParsing="false" onAction="#githubRedirectAction" text="GitHub" />
                  <MenuItem fx:id="menuDiscord" mnemonicParsing="false" onAction="#discordRedirectAction" text="Discord" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="406.0" prefWidth="677.0" VBox.vgrow="ALWAYS">
         <children>
            <ListView fx:id="listViewAddons" layoutX="14.0" layoutY="51.0" prefHeight="200.0" prefWidth="200.0" />
            <Text fx:id="textManagedLabel" layoutX="52.0" layoutY="37.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Managing (0) addons" wrappingWidth="118.0" />
            <Button fx:id="buttonRemove" layoutX="59.0" layoutY="260.0" mnemonicParsing="false" onAction="#removeAction" prefHeight="62.0" prefWidth="104.0" text="Remove" />
            <TextField fx:id="textFieldURL" layoutX="14.0" layoutY="349.0" prefHeight="32.0" prefWidth="487.0" promptText="Addon Url" />
            <Button layoutX="512.0" layoutY="347.0" mnemonicParsing="false" onAction="#addAction" prefHeight="32.0" prefWidth="104.0" text="Add" />
            <Button fx:id="buttonUpdate" layoutX="332.0" layoutY="260.0" mnemonicParsing="false" onAction="#updateAction" prefHeight="134.0" prefWidth="189.0" text="Update Addons" />
            <TextArea fx:id="textAreaOutputLog" editable="false" focusTraversable="false" layoutX="236.0" layoutY="51.0" prefHeight="200.0" prefWidth="380.0" />
            <Text layoutX="396.0" layoutY="37.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Output Log" />
            <ImageView fx:id="imageViewAdd" fitHeight="13.0" fitWidth="104.0" layoutX="512.0" layoutY="333.0" pickOnBounds="true" preserveRatio="true" />
            <Text fx:id="textAdd" layoutX="532.0" layoutY="338.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Processing..." visible="false" />
            <TextField fx:id="textFieldBranch" layoutX="14.0" layoutY="313.0" prefHeight="95.0" prefWidth="176.0" promptText="Github Branch" />
            <CheckBox fx:id="checkboxReleases" layoutX="198.0" layoutY="315.0" mnemonicParsing="false" onAction="#releasesAction" text="Use Releases" />
         </children>
    </AnchorPane>
  </children>
</VBox>
