<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="433.0" prefWidth="646.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Controller">
  <children>
    <MenuBar prefHeight="29.0" prefWidth="719.0" VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="File">
          <items>
            <MenuItem fx:id="menuSetup" mnemonicParsing="false" onAction="#setupAction" text="Setup" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem fx:id="menuPreferences" mnemonicParsing="false" text="Preferences" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem fx:id="menuGitHub" mnemonicParsing="false" text="GitHub" />
                  <MenuItem fx:id="menuDiscord" mnemonicParsing="false" text="Discord" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="406.0" prefWidth="677.0" VBox.vgrow="ALWAYS">
         <children>
            <ListView fx:id="listViewAddons" layoutX="14.0" layoutY="51.0" prefHeight="200.0" prefWidth="200.0" />
            <Text fx:id="textManagedLabel" layoutX="52.0" layoutY="37.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Managing (0) addons" wrappingWidth="118.0" />
            <Button fx:id="buttonRemove" layoutX="59.0" layoutY="260.0" mnemonicParsing="false" onAction="#removeAction" prefHeight="62.0" prefWidth="104.0" text="Remove" />
            <TextField fx:id="textFieldURL" layoutX="38.0" layoutY="351.0" prefHeight="38.0" prefWidth="464.0" />
            <Button layoutX="512.0" layoutY="346.0" mnemonicParsing="false" onAction="#addAction" prefHeight="49.0" prefWidth="104.0" text="Add" />
            <Button fx:id="buttonUpdate" layoutX="408.0" layoutY="84.0" mnemonicParsing="false" onAction="#updateAction" prefHeight="134.0" prefWidth="189.0" text="Update Addons" />
         </children>
    </AnchorPane>
  </children>
</VBox>
